# Copy this file to .env and fill in your actual values
# All variables must start with VITE_ to be available in the browser

# App Configuration
VITE_APP_NAME=HomeFresh

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain_here
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket_here
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id_here
VITE_FIREBASE_APP_ID=your_firebase_app_id_here

# WeChat OAuth Configuration (future implementation)
VITE_WECHAT_APP_ID=your_wechat_app_id_here
VITE_WECHAT_REDIRECT_URI=https://your-domain.com/wechat/callback

# API Configuration
VITE_API_BASE_URL=https://your-backend-api.com
